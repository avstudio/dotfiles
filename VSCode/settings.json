  "vim.foldfix": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["A", "A"],
      "after": ["<esc>", "A"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<esc>"],
      "commands": [":nohl", "removeSecondaryCursors"]
    },
    // {
    //   "before": [";"],
    //   "after": [":"]
    // },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.startFindReplaceAction"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.files.action.focusOpenEditorsView"]
    },
    {
      "before": ["<leader>", "["],
      "commands": [
        // "workbench.files.action.focusOpenEditorsView",
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "s"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-d>": true,
    "<C-q>": false
  },
  "search.runInExtensionHost": true,
  "vim.visualModeKeyBindings": [
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "s"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    // {
    //   "before": ["s"],
    //   "after": ["<leader>", "<leader>", "s"]
    // },
    {
      "before": ["<leader>", "8"],
      "commands": ["toggleFindCaseSensitive", "editor.action.changeAll"]
      // "commands": ["toggleFindCaseSensitive"]
    }
  ],
